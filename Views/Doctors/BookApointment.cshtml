@model List<Doctor>

<h1> Book Appointment</h1>
@for (int i = 0; i < Model.Count; i++)
{
    <div class="row">
        <div class="col-md-8">
            <div class="card " style="width: 18rem;">
        <img src="/images/@(i+1).jpg" width="50px" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">@Model[i].Name</h5>
            <p class="card-text"> Specialization:@Model[i].Specialization</p>
                    <a href="/Doctors/completeApointment?id=@(i+1)" class="btn btn-primary">complete Appointment</a>
            @*            <a href="/BookAppotiment/completeAppotiment?id" =@(i+1)">Get Details" class="btn btn-primary">Go somewhere</a>*@
        </div>
    </div>
        </div>
      
    </div>
   
}